{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"customTemplates": [
		{
			"name": "no-title",
			"postTypes": [
				"post",
				"page"
			],
			"title": "No title"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#000024",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#000000",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "lightgray",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "transparent",
					"name": "Transparent",
					"slug": "transparent"
				}
			]
		},
		"custom": {
			"button-border-color": "rgba(0, 0, 0, 0.05)",
			"button-border-radius": "4px",
			"button-highlight": "0 0 0 #fff",
			"button-padding": ".3rem 1rem",
			"button-shadow": "0 8px 8px 0 rgba(0, 0, 0, 0.2)",
			"h-spacing": "2.375rem",
			"small-button-shadow": "0 1px 1px 0 rgba(0, 0, 0, 0.2)"
		},
		"layout": {
			"contentSize": "1152px"
		},
		"spacing": {
			"units": [
				"rem"
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Space Grotesk\",sans-serif",
					"name": "System",
					"slug": "system"
				}
			],
			"fontSizes": [
				{
					"name": "Extra small",
					"size": "1rem",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "1.125rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "2rem",
					"slug": "large"
				},
				{
					"name": "XL",
					"size": "2.75rem",
					"slug": "x-large"
				},
				{
					"name": "XXL",
					"size": "3.75rem",
					"slug": "xx-large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"color": "var(--wp--custom--button-border-color)",
					"radius": "4px",
					"style": "solid",
					"width": "2px"
				},
				"color": {
					"background": "var(--wp--preset--color--foreground)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": {
						"bottom": "0.3rem",
						"left": "1rem",
						"right": "1rem",
						"top": "0.3rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			},
			"core/image": {
				"border": {
					"radius": ".25rem"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			}
		},
		"spacing": {
			"blockGap": ".5rem",
			"margin": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)",
			"fontSize": "1.25rem",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}